{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "java tmpl dev",
            "request": "launch",
            "mainClass": "java_tmpl.App",
            "projectName": "java_tmpl",
            "javaExec": "${workspaceFolder}/_inner/proj_tmpl_local/env/jdk/bin/java.exe",
            "cwd": "${workspaceFolder}/_inner/proj_tmpl_local/src/java_tmpl",
            "args": "--spring.profiles.active=dev"
        },
        {
            "type": "python",
            "name": "python tmpl",
            "request": "launch",
            "program": "app.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/_inner/proj_tmpl_local/src/python_tmpl",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/_inner/proj_tmpl_local/src/python_tmpl;${env:PYTHONPATH}"
            }
        }
    ]
}
